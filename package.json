{
  "name": "reactjspain-drawer",
  "version": "0.1.1",
  "description": "Redefined Drawer for React Apps",
  "main": "dist/drawer.js",
  "scripts": {
    "test": "cross-env NODE_ENV=testing jest",
    "example": "cross-env NODE_ENV=development webpack-dev-server --hot --config webpack.config.example.js",
    "build": "cross-env NODE_ENV=development webpack",
    "test:prod": "cross-env NODE_ENV=production jest",
    "example:prod": "cross-env NODE_ENV=production webpack --config webpack.config.example.js",
    "build:prod": "cross-env NODE_ENV=production && babel src --out-dir dist --source-maps --ignore src/example,src/tests && webpack -p",
    "prepublish": "npm run test:prod && npm run example:prod && npm run build:prod"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/reactjspain/drawer.git"
  },
  "keywords": [
    "react",
    "components",
    "react-component",
    "drawer",
    "react",
    "reactjspain"
  ],
  "author": "Jose Ramos <jlramosr@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/reactjspain/drawer/issues"
  },
  "homepage": "https://github.com/reactjspain/drawer#readme",
  "jest": {
    "transform": {
      ".*": "<rootDir>/node_modules/babel-jest"
    },
    "moduleFileExtensions": [
      "js"
    ]
  },
  "peerDependencies": {
    "react": "^16.2.0",
    "react-dom": "^16.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-jest": "^22.4.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "check-prop-types": "^1.1.2",
    "cross-env": "^5.1.3",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.18.1",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-react": "^7.7.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^2.30.1",
    "jest": "^22.4.0",
    "material-ui": "^1.0.0-beta.34",
    "material-ui-icons": "^1.0.0-beta.17",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-hot-loader": "^3.1.3",
    "react-test-renderer": "^16.2.0",
    "webpack": "^3.11.0",
    "webpack-cli": "^2.0.9",
    "webpack-dev-server": "^3.0.0"
  }
}
